{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly",
    "primitives": "readonly",
    "define": "readonly",
    "module": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "no-unused-vars": 0,
    "no-fallthrough": 0,
    "no-control-regex": 0,
    "no-empty": 0,
    "no-constant-condition": 0
  }
}